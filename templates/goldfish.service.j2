# CI/CD managed
# Do NOT modify this file by hand!

[Unit]
Description=Goldfish - hashicorp vault UI
After=network.target vault-server.service
Requires=network.target

[Service]
ExecStart={{goldfish_install_dir}}/goldfish -config={{goldfish_install_dir}}/goldfish_service.hcl

User={{goldfish_user}}
Group={{goldfish_user}}
Restart=always
RestartSec=5
TimeoutSec=5

[Install]
WantedBy=multi-user.target
